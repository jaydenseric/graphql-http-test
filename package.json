{
  "name": "graphql-http-test",
  "version": "1.0.0-alpha.1",
  "description": "A JavaScript API and CLI to test a GraphQL server for GraphQL HTTP spec compliance.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/graphql-http-test",
  "homepage": "https://github.com/jaydenseric/graphql-http-test",
  "bugs": "https://github.com/jaydenseric/graphql-http-test/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "test",
    "graphql",
    "http",
    "standard",
    "standards",
    "spec",
    "specification",
    "compliance"
  ],
  "files": [
    "cli",
    "lib"
  ],
  "main": "lib",
  "bin": {
    "graphql-http-test": "cli/graphql-http-test.js"
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "graphql": "0.13.1 - 14"
  },
  "dependencies": {
    "isobject": "^4.0.0",
    "node-fetch": "^2.6.0",
    "test-director": "^4.0.0"
  },
  "devDependencies": {
    "coverage-node": "^2.0.3",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-env": "^13.0.4",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql": "^14.6.0",
    "graphql-api-koa": "^4.1.1",
    "hard-rejection": "^2.1.0",
    "jsdoc-md": "^5.0.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "prettier": "^2.0.2",
    "snapshot-assertion": "^1.0.0"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:eslint": "eslint .",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:api": "coverage-node -r hard-rejection/register test",
    "prepublishOnly": "npm test"
  }
}
